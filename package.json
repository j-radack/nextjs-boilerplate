    "react-dom": "^18.2.0",
    "react-spinners": "0.13.8",
    "sass": "^1.71.1",
    "server-only": "^0.0.1",
    "sharp": "^0.32.6",
    "sweetalert2": "11.10.5",
    "zod": "3.22.4",
