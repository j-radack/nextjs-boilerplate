{
  "name": "next-core-project",
  "version": "2.0.0",
  "private": false,
  "author": "https://duongnamtruong.com",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "create-network": "docker network create my_network",
    "run:dev": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose -f docker-compose.dev.yml build --parallel && docker-compose -f docker-compose.dev.yml up",
    "run:prod": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 docker-compose -f docker-compose.prod.yml build --parallel && docker-compose -f docker-compose.prod.yml up -d",
    "stop-all-running-containers": "docker kill $(docker ps -q) && docker rm $(docker ps -a -q)",
    "free-space": "docker system prune -af --volumes",
    "test": "vitest --ui",
    "coverage": "vitest --coverage"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@tanstack/react-query": "^4.29.14",
    "antd": "^5.6.2",
    "axios": "^1.4.0",
    "clsx": "^1.2.1",
    "cookies-next": "^2.1.2",
    "framer-motion": "^10.12.16",
    "immer": "^10.0.2",
    "next": "^13.4.6",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.63.4",
    "server-only": "^0.0.1",
    "sharp": "^0.32.1",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@next/bundle-analyzer": "^13.4.6",
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@tanstack/react-query-devtools": "^4.29.14",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.3.1",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.13",
    "@vitejs/plugin-react": "^4.0.1",
    "@vitest/coverage-v8": "^0.32.2",
    "@vitest/ui": "^0.32.2",
    "chalk": "^5.2.0",
    "eslint": "8.43.0",
    "eslint-config-next": "^13.4.6",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.2"
  }
}
